{
  "type": "coupled_solvers.models.analytical_1d",
  "settings": {
    "delta_t": 0.0001,
    "timestep_start": 0,
    "solver_models": [
      {
        "type": "solver_wrappers.python.tube_flow_solver",
        "settings": {
          "working_directory": "CFD",
          "input_file": "solver_parameters.json",
          "interface_input": [
            {
              "model_part": "wall",
              "variables": [
                "displacement"
              ]
            }
          ],
          "interface_output": [
            {
              "model_part": "wall",
              "variables": [
                "pressure",
                "traction"
              ]
            }
          ]
        }
      },
      {
        "type": "solver_wrappers.python.tube_structure_solver",
        "settings": {
          "working_directory": "CSM",
          "input_file": "solver_parameters.json",
          "interface_input": [
            {
              "model_part": "wall",
              "variables": [
                "pressure",
                "traction"
              ]
            }
          ],
          "interface_output": [
            {
              "model_part": "wall",
              "variables": [
                "displacement"
              ]
            }
          ]
        }
      }
    ]
  }
}