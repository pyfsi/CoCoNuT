FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
	 outlet
	 {
		 type          slip;
	 }

	 inlet
	 {
		 type          slip;
	 }
    dieWall
	 {
		 type          fixedValue;
		 value         uniform (0 0 0);
	 }

	back
    {
        type            wedge;
    }

    front
    {
        type            wedge;
    }



 
 	 upWire 
	 { 
		 type  	 fixedValue; 
		 value	 uniform (0.0 0.0 0.0);
	 } 
 
 	 deformWire 
	 { 
		 type  	 fixedValue; 
		 value	 uniform (0.0 0.0 0.0);
	 } 
 
 	 finalWire 
	 { 
		 type  	 fixedValue; 
		 value	 uniform (0.0 0.0 0.0);
	 } 
} 

