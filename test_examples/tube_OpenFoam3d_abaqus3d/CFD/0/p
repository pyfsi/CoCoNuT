FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            uniformTotalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              table
        
4
(
(0 1.3332)
(0.003 1.3332)
(0.0031 0)
(0.01 0)
)
;
        value           uniform 1.3332;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    mantle
    {
        type            zeroGradient;
    }
}


