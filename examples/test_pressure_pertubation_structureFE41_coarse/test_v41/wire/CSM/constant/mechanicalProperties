FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    location    "constant";
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

mechanical
{
    type            multiMaterial;

    laws
    {
        wire
        {
	//Solve a Poisson equation to calculate the hydrostatic sigma pressure
	//field
	
	   solvePressureEqn yes;

	//Scale the amount of smoothing applied to the pressure field
	
	//Defaults to 100

	//Set larger values (e;g. 1e4, 1e5, 1e6) will resulting in an increasingly smooth pressure field
	//When using values close to 1.0 or less, oscillations may start to appear

	  pressureSmoothingScaleFactor 100;

	//Mises/J2 with linear isotropic hardening
		
            type        elastoPlasticKirchhoff;
            rho         rho [ 1 -3 0 0 0 0 0 ] 7833;
            E           E [ 1 -1 -2 0 0 0 0 ] 1.77e+11;
            nu          nu [ 0 0 0 0 0 0 0 ] 0.3;
            initialYieldStress initialYieldStress [ 1 -1 -2 0 0 0 0] 1250e+06;
            hardeningCoefficient hardeningCoefficient [ 1 -1 -2 0 0 0 0] 300e+06;
        }
    }
}


// ************************************************************************* //
