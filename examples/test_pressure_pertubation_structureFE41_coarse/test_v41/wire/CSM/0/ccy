FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      ccy;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   nonuniform List<scalar> 6(0.000499524 0.000499524 0.000499524 0.000499524 0.000499524 0.000499524);

boundaryField
{
    wireDownstream
    {
        type            calculated;
        value           uniform 0.000499524;
    }
    wireUpstream
    {
        type            calculated;
        value           uniform 0.000499524;
    }
    wireTopContact
    {
        type            calculated;
        value           nonuniform List<scalar> 6(0.000749286 0.000749286 0.000749286 0.000749286 0.000749286 0.000749286);
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


