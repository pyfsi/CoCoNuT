FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "DU|diffusionT"
    {
		tolerance       1e-18;
		relTol          0;
		minIter         1;
		solver          PCG;
		preconditioner  DIC;
    }
}

solidMechanics
{
    // Maximum number of momentum correctors
    nCorrectors            10000;

    // Solution tolerances
    DU                     1e-8;
    relDU                  1e-8;
    materials              1e-8;

    // Frequency of writing the residuals
    infoFrequency          100;

    // Enable thermal stress
    thermalStress          no;
    
}

relaxationFactors
{
    equations
    {
       "DU.*"           1;
    }
}
