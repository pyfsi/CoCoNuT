FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver           GAMG;
        tolerance        1e-10;
        relTol           1e-5;

        smoother         DICGaussSeidel;

    }

    pFinal
    {
        $p;
        relTol          0;
    }

    U
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-10;
        relTol          1e-5;
    }

    UFinal
    {
        $U;
        relTol          0;
    }

    "cellDisplacement"
    {
    	solver          GAMG;
    	smoother        GaussSeidel;
    	tolerance       1e-10;
    	relTol          0.001;
    }
}

PIMPLE
{
    nNonOrthogonalCorrectors 0;
    nCorrectors         3;
    nOuterCorrectors 50;
    residualControl
    {
        U
        {
                tolerance  1e-8;
                relTol      0;
        }
        p
        {
                tolerance  1e-7;
                relTol      0;
        }
     }
}


relaxationFactors
{
 fields
 {
        p               0.7;
 }
 equations
 {
        U               0.7;
 }
}
