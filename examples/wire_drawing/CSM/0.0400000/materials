FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.0400000";
    object      materials;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wireUpstream
    {
        type            zeroGradient;
    }
    wireDownstream
    {
        type            zeroGradient;
    }
    wireTopContact
    {
        type            zeroGradient;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


