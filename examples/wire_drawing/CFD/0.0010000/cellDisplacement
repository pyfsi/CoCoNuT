FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.0010000";
    object      cellDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    dieWall
    {
        type            cellMotion;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            slip;
    }
    Wire
    {
        type            cellMotion;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            slip;
    }
    back
    {
        type            wedge;
    }
    front
    {
        type            wedge;
    }
}


