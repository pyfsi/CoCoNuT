FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      DU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform ( 0 0 0 );

boundaryField
{
    wireDownstream
    {
	displacementSeries
	{
		fileName	"$FOAM_CASE/constant/timeVsDisp";
                outOfBounds     clamp;    
	}
 	
	type		fixedDisplacementZeroShear;
        refValue	uniform (0 0 0 ); 
 	refGradient	uniform (0 0 0 );
	valueFriction	uniform (1 0 0 0 0 0 );
	value		uniform (0 0 0);	
    }

    wireUpstream
    {
        fileName	"$FOAM_CASE/constant/timeVsDownstreamPressure";
        outOfBounds	clamp;
        normal		(1 0 0);
	updateTangentialDisplacement	no;
	relaxationFactor	0.025;
    	type            fixedTangentialNormalPressure;
        refValue        uniform ( 0 0 0 );
        refGradient	uniform ( 0 0 0 );
	valueFraction	uniform ( 0 -0 -0 1 0 1 );
        value           uniform (0 0 0);
    }

    wireTopContact
    {
        type           	timeVaryingMappedSolidTraction;
        traction	uniform (0 0 0 );
	presure		uniform 0.0;
	value		uniform (0 0 0 );
	findSampleTimeMultiple	false;
    }


   front
    {
       type          solidWedge;
        patchType     wedge;
        value         uniform (0 0 0);
    }

    back
    {
       type          solidWedge;
        patchType     wedge;
        value         uniform (0 0 0);
    }


}


// ************************************************************************* //
