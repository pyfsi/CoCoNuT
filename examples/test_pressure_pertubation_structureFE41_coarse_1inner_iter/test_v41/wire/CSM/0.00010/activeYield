FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.00010";
    object      activeYield;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   nonuniform List<scalar> 6(0 0 1 1 1 1);

boundaryField
{
    wireDownstream
    {
        type            calculated;
        value           uniform 0;
    }
    wireUpstream
    {
        type            calculated;
        value           uniform 1;
    }
    wireTopContact
    {
        type            calculated;
        value           nonuniform List<scalar> 6(0 0 1 1 1 1);
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


