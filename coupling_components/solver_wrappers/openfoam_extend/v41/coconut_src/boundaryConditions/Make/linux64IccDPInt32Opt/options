ifeq ($(WM_PROJECT), foam)
    VER := $(shell expr `echo $(WM_PROJECT_VERSION)` \>= 4.1)
    ifeq ($(VER), 1)
        VERSION_SPECIFIC_INC = -DFOAMEXTEND=41
    else
        VERSION_SPECIFIC_INC = -DFOAMEXTEND=40
    endif
else
    VERSION_SPECIFIC_INC = -DOPENFOAMESIORFOUNDATION -I$(LIB_SRC)/dynamicFvMesh/lnInclude/
    ifneq (,$(findstring v,$(WM_PROJECT_VERSION)))
        VERSION_SPECIFIC_INC += -DOPENFOAMESI
    else
        VERSION_SPECIFIC_INC += -DOPENFOAMFOUNDATION
    endif
endif
EXE_INC = -Wno-old-style-cast -Wno-attributes -DExtendSpecific $(VERSION_SPECIFIC_INC) -I../meshSmoothers/lnInclude -I../processLine/lnInclude -I../../ThirdParty/cfmesh/meshLibrary/lnInclude -I../../ThirdParty/eigen3 -I../../ThirdParty/gsl2 -I../../ThirdParty/FADBAD -I$(LIB_SRC)/finiteVolume/lnInclude -I$(LIB_SRC)/meshTools/lnInclude -I$(LIB_SRC)/finiteArea/lnInclude -I$(LIB_SRC)/lagrangian/lnInclude -I$(LIB_SRC)/dynamicMesh/lnInclude -I$(LIB_SRC)/dynamicMesh/dynamicMesh/lnInclude -I$(LIB_SRC)/dynamicMesh/dynamicFvMesh/lnInclude -I$(LIB_SRC)/dynamicMesh/topoChangerFvMesh/lnInclude -I$(LIB_SRC)/sampling/lnInclude -I$(LIB_SRC)/surfMesh/lnInclude -I$(LIB_SRC)/postProcessing/foamCalcFunctions/lnInclude -I$(LIB_SRC)/errorEstimation/lnInclude $(METIS_INCLUDE_DIR) -I$(LIB_SRC)/decompositionMethods/decompositionMethods/lnInclude -I$(LIB_SRC)/decompositionMethods/metisDecomp/lnInclude -I$(LIB_SRC)/decompositionMethods/scotchDecomp/lnInclude -I$(LIB_SRC)/mesh/extrudeModel/lnInclude -I$(FOAM_UTILITIES)/mesh/generation/extrudeMesh -I$(FOAM_UTILITIES)/mesh/generation/extrudeMesh/extrudedMesh -I$(FOAM_UTILITIES)/preProcessing/mapFields
